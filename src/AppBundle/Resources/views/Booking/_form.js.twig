$(document).ready(function(){
    $('body').on('submit', 'form[name=booking_form]', function(){
        $.ajax({
            type: "POST",
            url: "{{ path('create_booking') }}",
            data: $(this).serialize(),
            success: function(data) {
            alert(data);
            if (data == "{{ success }}") {
                window.location = window.location;
            }
            },
            error:  function(xhr, str){
            alert("Возникла ошибка!");
            }
            });
        });
    }
);
